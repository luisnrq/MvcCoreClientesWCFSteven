@model Coche[] 

<table class="table table-active table-hover">
    <thead>
        <tr>
            <th>
                Coche
            </th>
            <th>
                Imagen
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(Coche car in Model)
            {
                <tr>
                    <td>@car.Modelo</td>
                    <td><img src="@car.Imagen" style="width:150px;height:150px" /></td>
                    <td><a asp-controller="Coches" asp-action="Details" asp-route-id="@car.IdCoche">Details</a></td>
                </tr>
            }
    </tbody>
</table>